{
  "dependencies": {
    "@tailwindcss/cli": "^4.0.9",
    "tailwindcss": "^4.0.9",
    "upgrade": "^1.1.0"
  },
  "devDependencies": {
    "livereload": "^0.9.3",
    "prettier": "^3.3.3",
    "prettier-plugin-jinja-template": "^2.0.0"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-jinja-template"
    ],
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "jinja-template"
        }
      }
    ]
  }
}
