{
  "devDependencies": {
    "prettier": "^3.3.3",
    "prettier-plugin-jinja-template": "^1.4.1",
    "tailwindcss": "^3.4.10"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-jinja-template"
    ],
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "jinja-template"
        }
      }
    ]
  }
}
