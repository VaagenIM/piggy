{
  "dependencies": {
    "tailwindcss": "^3.4.10"
  },
  "devDependencies": {
    "livereload": "^0.9.3",
    "prettier": "^3.3.3",
    "prettier-plugin-jinja-template": "^2.0.0"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-jinja-template"
    ],
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "jinja-template"
        }
      }
    ]
  }
}
